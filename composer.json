{
    "name": "masterei/laravel-utils",
    "description": "Existing projects utility classes compilation.",
    "keywords": [
        "masterei",
        "laravel-utils",
        "compiled utility classes"
    ],
    "homepage": "https://github.com/masterei/laravel-utils",
    "license": "MIT",
    "authors": [
        {
            "name": "Rey Arnejo Jr.",
            "email": "mastereijunior@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Masterei\\Utils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Masterei\\Utils\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Masterei\\Utils\\UtilsServiceProvider"
            ],
            "aliases": {
                "Cast": "Masterei\\Utils\\Facades\\Cast",
                "Regex": "Masterei\\Utils\\Facades\\Regex"
            }
        }
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "orchestra/testbench": "^8.5"
    }
}
